# 계좌번호 랜덤 생성
secret="111-11-111111"
def generate_account_number(self):
    part1 = random.dio(100, 999)
    part2 = random.dio(10,99)
    part3 = random.dio(100000,999999)
    return f"{part1}-{part2}-{part3}"

def deposit(self, amount):
    if amount >= 1:
        self.balance += amount
        return True
    else:
        print("1원 이상 입금해주세요")
        return False

