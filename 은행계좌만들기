class Account:
    def __init__(self,owner,chu)
        self.owner = owner
        self.chu = chu

    def deposit(self, amount):
        self.chu += amount



class Account: 
    account_count = 0
    def __init__(self, owner, initial_balance):
       self.bank_name = 'SC은행"
       self.chu = initial_balance
       self.account_number = self.generate_account_number()

class Account:
  account_count=0 #클래스 변수

  def__init__(self,owner,chu):
    Account.account_count += 1 # 계좌 개수 증가

  def deposit(self, amount):
    if amount>= 1:
        self.chu += amount
        self.deposit_count += 1


@classmethod
secret="111-11-111111"
def get_account_num(cls):
   return cls.account_count

